{"ast":null,"code":"import { returnStatement } from '@babel/types';\nexport default {\n  name: \"headerNavigation\",\n\n  data() {\n    return {\n      scrolledNav: null,\n      mobile: null,\n      mobileNav: null,\n      windowWidth: null\n    };\n  },\n\n  created() {\n    window.addEventListener(\"resize\", this.checkScreen);\n    this.checkScreen();\n  },\n\n  mounted() {\n    window.addEventListener(\"scroll\", this.updateScroll);\n  },\n\n  methods: {\n    toggleMobileNav() {\n      this.mobileNav = !this.mobileNav;\n    },\n\n    updateScroll() {\n      const scrollPosition = window.scrollY;\n\n      if (scrollPosition > 50) {\n        this.scrolledNav = true;\n        return;\n      }\n\n      this.scrolledNav = false;\n    },\n\n    checkScreen() {\n      this.windowWidth = window.innerWidth;\n\n      if (this.windowWidth <= 750) {\n        this.mobile = true;\n        return;\n      }\n\n      this.mobile = false;\n      this.mobileNav = false;\n      returnStatement;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoCA,SAASA,eAAT,QAAgC,cAAhC;AAGA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,IADR;MAELC,MAAM,EAAE,IAFH;MAGLC,SAAS,EAAE,IAHN;MAILC,WAAW,EAAE;IAJR,CAAP;EAMD,CATY;;EAUbC,OAAO,GAAG;IACRC,MAAM,CAACC,gBAAP,CAAwB,QAAxB,EAAkC,KAAKC,WAAvC;IACA,KAAKA,WAAL;EACD,CAbY;;EAcbC,OAAO,GAAG;IACRH,MAAM,CAACC,gBAAP,CAAwB,QAAxB,EAAkC,KAAKG,YAAvC;EACD,CAhBY;;EAkBbC,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,KAAKT,SAAL,GAAiB,CAAC,KAAKA,SAAvB;IACD,CAHM;;IAKPO,YAAY,GAAG;MACb,MAAMG,cAAa,GAAIP,MAAM,CAACQ,OAA9B;;MACA,IAAID,cAAa,GAAI,EAArB,EAAyB;QACvB,KAAKZ,WAAL,GAAmB,IAAnB;QACA;MACF;;MACA,KAAKA,WAAL,GAAmB,KAAnB;IACD,CAZM;;IAcPO,WAAW,GAAG;MACZ,KAAKJ,WAAL,GAAmBE,MAAM,CAACS,UAA1B;;MACA,IAAI,KAAKX,WAAL,IAAoB,GAAxB,EAA4B;QAC1B,KAAKF,MAAL,GAAc,IAAd;QACA;MACF;;MACA,KAAKA,MAAL,GAAc,KAAd;MACA,KAAKC,SAAL,GAAiB,KAAjB;MACAL,eAAe;IAChB;;EAvBM;AAlBI,CAAf","names":["returnStatement","name","data","scrolledNav","mobile","mobileNav","windowWidth","created","window","addEventListener","checkScreen","mounted","updateScroll","methods","toggleMobileNav","scrollPosition","scrollY","innerWidth"],"sourceRoot":"","sources":["/Users/Loredana/Documents/Groupomania/frontend/src/App.vue"],"sourcesContent":["<template>\n  <header :class=\"{ 'scrolled-nav': scrolledNav}\">\n    <nav>\n      <div class=\"brand\">\n        <img src=\"@/assets/icon-above-font.png\" alt=\"\">\n      </div>\n      <ul v-show=\"!mobile\" class=\"navigation\">\n        <li><router-link class=\"link\" :to=\"{ name:''}\" >Home</router-link> </li>\n       <li><router-link class=\"link\" :to=\"{ name:''}\">Profile</router-link> </li>\n       <li><router-link class=\"link\" :to=\"{ name:''}\">Login</router-link> </li>\n       <li><router-link class=\"link\" :to=\"{ name:'signUp'}\">Sign Up</router-link> </li>\n      </ul>\n      <!--<div class=\"icon\">\n       <i @click=\"toggleMobileNav\" v-show=\"mobile\" class=\"fa-solid fa-align-justify\" :class=\"{'icon-active' : mobileNav}\"></i>\n      </div>-->\n        \n      <transition name=\"mobile-nav\">\n      <ul v-show=\"mobileNav\" class=\"dropdown-nav\">\n        <li><router-link class=\"link\" :to=\"{ name:''}\">Home</router-link> </li>\n        <li><router-link class=\"link\" :to=\"{ name:''}\">Profile</router-link> </li>\n        <li><router-link class=\"link\" :to=\"{ name:''}\">Login</router-link> </li>\n        <li><router-link class=\"link\" :to=\"{ name:''}\">Sign Up</router-link> </li>\n      </ul>\n      </transition>\n    </nav>\n    <router-view/>\n  </header>\n</template>\n<!--<nav>\n    <router-link to=\"/\">Sign Up</router-link> |\n  </nav>\n  <router-view/>\n</template>-->\n  \n\n<script>\nimport { returnStatement } from '@babel/types';\n\n\nexport default {\n  name: \"headerNavigation\",\n  data() {\n    return {\n      scrolledNav: null,\n      mobile: null,\n      mobileNav: null,\n      windowWidth: null,\n    };\n  },\n  created() {\n    window.addEventListener(\"resize\", this.checkScreen);\n    this.checkScreen();\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.updateScroll);\n  },\n\n  methods: {\n    toggleMobileNav() {\n      this.mobileNav = !this.mobileNav;\n    },\n\n    updateScroll() {\n      const scrollPosition = window.scrollY;\n      if (scrollPosition > 50) {\n        this.scrolledNav = true;\n        return;\n      }\n      this.scrolledNav = false;\n    },\n\n    checkScreen() {\n      this.windowWidth = window.innerWidth;\n      if (this.windowWidth <= 750){\n        this.mobile = true;\n        return;\n      }\n      this.mobile = false;\n      this.mobileNav = false;\n      returnStatement;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\nheader {\n  background-color: white;\n  z-index: 99;\n  width: 100%;\n  position: fixed;\n  transition: 0.5s ease all;\n  color: red;\n\n  nav {\n    position: relative;\n    display: flex;\n    flex-direction: row;\n    padding: 12px 0;\n    transition: .5s ease all;\n    width: 90%;\n    margin: 0 auto;\n    @media(min-width: 1140px) {\n      max-width: 1140px;\n    }\n\n    ul,\n    .link {\n      font-weight: 500;\n      color: red;\n      list-style: none;\n      text-decoration: none;\n    }\n\n    li{\n      text-transform: uppercase;\n      padding: 16px;\n      margin-left: 16px;\n    }\n\n    .link {\n      font-size: 14px;\n      transition: .5s ease all;\n      padding-bottom: 4px;\n      border-bottom: 1px solid transparent;\n\n      &:hover {\n        color: #00afea;\n        border-color: #00afea;\n      }\n    }\n\n    .brand {\n      display: flex;\n      align-items: center;\n\n\n      img {\n        width: 80px;\n        transition: 0.5s ease all;\n      }\n    }\n\n    .navigation {\n      display: flex;\n      align-items: center;\n      flex: 1;\n      justify-content: flex-end;\n    }\n    .icon {\n      display: flex;\n      align-items: center;\n      position: absolute;\n      top: 0;\n      right: 24px;\n      height: 100%;\n      \n      i {\n        cursor: pointer;\n        font-size: 24px;\n        transition: 0.8s ease all;\n      }\n    }\n\n    .icon-active {\n      transform: rotate(180deg);\n    }\n\n    .dropdown-nav {\n      display: flex;\n      flex-direction: column;\n      position: fixed;\n      width: 100%;\n      max-width: 250px;\n      height: 100%;\n      background-color: #fff;\n      top: 0;\n      left: 0;\n\n      li {\n        margin-left: 0;\n        .link {\n          color: black;\n        }\n      }\n    }\n    /* effect for transition of the meniu */\n    .mobile-nav-enter-active,\n    .mobile-nav-leave-active {\n      transition: 1s ease all;\n    }\n\n    .mobile-nav-enter-from,\n    .mobile-nav-leave-to {\n      transform: translateX(-250px);\n    }\n\n    .mobile-nav-enter-to {\n      transform: translateX(0);\n    }\n  }\n}\n \n\n.scrolled-nav {\n background-color: whitesmoke;\n box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);\n\n  nav {\n    padding:  8px 0;\n\n    .brand {\n      img {\n        width: 70px;\n        box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);\n      }\n    }\n  }\n}\n</style>\n\n\n<!--<style>\n\n\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\nnav {\n  padding: 30px;\n}\n\nnav a {\n  font-weight: bold;\n  color: #2c3e50;\n}\n\nnav a.router-link-exact-active {\n  color: #42b983;\n}\n</style>-->\n\n\n"]},"metadata":{},"sourceType":"module"}